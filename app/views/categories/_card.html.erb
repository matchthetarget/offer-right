<div class="card mb-3">
  <h2 class="card-header h4">
    Category #<%= category.id %>
  </h2>

  <div class="card-body">
    <dl class="mb-1">
      <dt>Name</dt>
      <dd><%= category.name %></dd>

    </dl>
  </div>

  <div class="card-footer text-muted">
    Last updated <%= time_ago_in_words(category.updated_at) %> ago
  </div>
</div>
